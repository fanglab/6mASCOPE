            "format": "{:,d}",
            "id": "nbases",
            "name": "Polymerase Read Bases",
            "description": "Number of bases in the polymerase reads"
        },
        {
            "type": "int",
            "format": "{:,d}",
            "id": "nreads",
            "name": "Polymerase Reads",
            "description": "Number of polymerase reads"
        },
        {
            "type": "int",
            "format": "{:,d}",
            "id": "read_n50",
            "name": "Polymerase Read N50",
            "description": "The polymerase read length at which 50% of the polymerase read bases are in polymerase reads longer than, or equal to, this value"
        },
        {
            "type": "int",
            "format": "{:,d}",
            "id": "read_length",
            "name": "Polymerase Read Length (mean)",
            "description": "The mean polymerase read length"
        },
        {
            "type": "int",
            "format": "{:,d}",
